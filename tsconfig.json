{
   "compilerOptions": {
      // "lib": [
      //    "ES2015"
      // ],
      "strictNullChecks": true,
      "target": "ES2022",
      "module": "ES2022",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
   },
   "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
}
}